{
  "name": "reactjs",
  "version": "1.0.0",
  "description": "React JS course",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot --env.NODE_ENV=development",
    "build": "webpack --mode production --env.NODE_ENV=production",
    "build-dev": "webpack --mode development --env.NODE_ENV=development",
    "test": "jest --watchAll",
    "test-c": "jest --watchAll --coverage --collectCoverageFrom=src/**/*.js",
    "snap-update": "jest --updateSnapshot",
    "cypress:open": "cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kipps/reactjs-epam.git"
  },
  "author": "Roman Shevchenko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kipps/reactjs-epam/issues"
  },
  "homepage": "https://github.com/kipps/reactjs-epam#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "express": "^4.16.4",
    "isomorphic-fetch": "^2.2.1",
    "jest-plugin-context": "^2.9.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^5.0.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-form": "^8.2.0",
    "redux-loop": "^4.5.4",
    "redux-promise": "^0.6.0",
    "redux-saga": "^1.0.2",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.2.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "deep-freeze": "0.0.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.3.1",
    "jest-css-modules": "^1.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.12.0",
    "react-addons-test-utils": "^15.6.2",
    "redux-devtools": "^3.5.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "jest": {
    "setupFiles": [
      "./__tests__/global.js",
      "jest-plugin-context/setup"
    ],
    "modulePathIgnorePatterns": [
      "global.js"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "testMatch": [
      "**/__tests__/components/?(*.)(spec|test).js?(x)"
    ]
  }
}
