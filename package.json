  "dependencies": {
    "bootstrap": "^4.3.1",
    "deep-freeze": "0.0.1",
    "express": "^4.16.4",
    "identity-obj-proxy": "^3.0.0",
    "react": "^15.6.2",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dom": "^15.6.2",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
